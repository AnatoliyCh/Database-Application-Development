<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="LR2"
                   namespace="LR2.Model.Table">
  <class name="Actor" table="Actors">
    <id name ="Id">
      <generator class="sequence">
        <param name="sequence">Actors_sequence</param>
      </generator>
    </id>
    <property name="Name" unique-key="Name"  column="Имя"/>
  </class>
</hibernate-mapping>