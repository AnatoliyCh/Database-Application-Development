<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="LR2"
                   namespace="LR2.Model.Table">
  <class name="Film" table="Films">
    <id name ="Id">
      <generator class="sequence">
        <param name="sequence">Films_sequence</param>
      </generator>
    </id>
    <property name="Title" column="Название"/>
    <property name="Rating" column="Рейтинг"/>
    <property name="Viewed" column="Просмотрен"/>
  </class>
</hibernate-mapping>